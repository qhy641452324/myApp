<ion-view view-title="User" hide-nav-bar="true">
	<ion-header-bar class="bar-light">
		<button class="button button-icon icon ion-ios-arrow-left" ng-click="$ionicGoBack()"></button>
		<h1 class="title">绑定手机号</h1>
	</ion-header-bar>
	<ion-content scroll="true" has-bouncing="true">
		<div class="list bindPhone">
			<input class="item" placeholder="请输入密码" ng-model="setPassword.password">
			<input class="item" placeholder="请再次输入密码"  ng-model="setPassword.passwordAgain">
		</div>
		<button class="button  button-assertive bindPhoneBut" ng-click="SetPassword()">
				确定
	    </button>
	</ion-content>
</ion-view>