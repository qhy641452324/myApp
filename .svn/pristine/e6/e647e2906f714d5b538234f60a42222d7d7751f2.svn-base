<ion-view view-title="User" hide-nav-bar="true">
	<ion-header-bar class="bar-light">
		<button class="button button-icon icon ion-ios-arrow-left" ng-click="$ionicGoBack()"></button>
		<h1 class="title">更换手机号</h1>
	</ion-header-bar>
	<ion-content scroll="true" has-bouncing="true">
		<div class="list bindPhone">
			<input class="item" placeholder="请输入当前手机号">
			<input class="item" placeholder="请输入新手机号">
			<input class="item" placeholder="请输入验证码">
			<span class="changePhoneCode">获取验证码</span>
		</div>
		<button class="button button-block bindPhoneBut" ui-sref="tab.setPassword">
			完成
	    </button>
	</ion-content>
</ion-view>