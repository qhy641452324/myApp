<ion-view view-title="User" hide-nav-bar="true">
	<ion-header-bar class="bar-light">
		<button class="button button-icon icon ion-ios-arrow-left" ng-click="$ionicGoBack()"></button>
		<h1 class="title">拼团消息</h1>
	</ion-header-bar>
	<ion-content scroll="true" has-bouncing="true" class="content">
		<ul>
			<li class="bibuMessageContent" ng-repeat="bibuMessageItem in bibuMessageList">
				<span class="time">
					{{bibuMessageItem.insertTimeStr}}
				</span>
				<div class="item item-icon-right fightGroupsMessageCard">
					<h3>拼团成功通知</h3>
					<p class="fightGroupstext redColor">{{bibuMessageItem.messageTitle}}</p>
					<div class="groupContent">
						<p>{{bibuMessageItem.contactUser}}</p>
						<p>团长：{{bibuMessageItem.messageTitle}}</p>
						<p>成团人数：{{bibuMessageItem.groupMemberLimit}}</p>
						<p>订单号：{{bibuMessageItem.idOrder}}</p>
					</div>
					<p class="groupBottom">点击查看详情>>></p>
				</div>
			</li>
		</ul>
	</ion-content>
</ion-view>