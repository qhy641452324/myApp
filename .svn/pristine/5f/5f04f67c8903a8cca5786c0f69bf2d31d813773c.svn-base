//giftMygift
.giftMygift{
    .bar-header{
        border-bottom: 1px solid #F5F5F5;
    }
    .tabbed-slidebox .tsb-icons{
        margin-top: 44px;
        a{
            width: 50%;
            line-height: 2.5rem;
         }
         a.active:after{
             left: 50%;
             bottom: 0;
             margin-left: -10px;
         }
    } 
    
    .tabbed-slidebox .slider-slide{
        .scroll{
            padding-top: 0;
        }
    }
    .peopleInfo{
        min-height: 250px;
        width: 100%;
        position: relative;
        img{
            width: 71px;
            height: 71px;
            position: absolute;
            top: 10px;
            left: 50%;
            margin-left: -35px;
            border-radius: 35px;
            border: 3px solid #fff;
        }
        .userName{
            position: absolute;
            top: 91px;
            width: 100%;
            text-align: center;
            font-size: 1rem;
        }
        .giftCount{
            width: 100%;
            top: 148px;
            position: relative;
            height: auto;
            p{
                width: 50%;
                float: left;
                font-size: 2rem;
                line-height: 1.5rem;
                font-weight: 600;
                span{
                    font-size: 0.8rem;
                    font-weight: 200;
                    color: #666;
                }
            }
        }
         .yearInput{
            position: absolute;
            right: 20px;
            top: 20px;
            .icon{
                position: absolute;
                bottom: -6px;
                left: 50%;
                margin-left: -14px;
                color: #FE4C40;
            }
        }
        .giftYear{
            width: 53px;
            height: 22px;
            right: 20px;
            top: 20px;
            color: #FE4C40;
            background: #f5f5f5;
            border: none;
            font-size: 1rem;
            
        }
       
        input::-webkit-input-placeholder{ /*WebKit browsers*/
            color: #FE4C40;
        }
            input::-moz-input-placeholder{ /*Mozilla Firefox*/
            color: #FE4C40;
        }
            input::-ms-input-placeholder{ /*Internet Explorer*/ 
            color: #FE4C40;
        }
    }
    .list{
        .item{
            text-align: left;
            min-height: 60px;
            padding-top: 5px;
             padding-bottom: 5px;
             h2{
                 padding: 3px 0;
             }
            img{
                top: 5px;
                max-height: 50px;
                max-width: 50px;
            }
            div{
                position: absolute;
                right: 8px;
                top: 8px;
                text-align: right;
                .money{
                    color: #000;
                    font-weight: 400;
                    font-size: 1rem;
                }
                p{
                    font-size: 0.8rem;
                }
            }
        }
    }
    .sendGiftList{
        .peopleInfo{
            min-height: 150px;
        }
        .item-avatar{
            padding-left: 10px;
            padding-right: 25px;
            div{
                right: 25px;
                p:nth-child(2){
                    color: #999;
                }
            }
            .icon{
                position: absolute;
                right: 0;
                top: 50%;
                color: #CACACA;
                margin-top: -8px;
                margin-right: 5px;
            }
        }
    }
    .listEmpty{
        background: #F5F5F5;
        width: 100%;
        text-align: center;
        color: #A5A5A5;
        position: absolute;
        bottom: -16rem;
    }
   
}

